{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{html,js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acmilank22/react-app-boilerplate.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acmilank22/react-app-boilerplate/issues"
  },
  "homepage": "https://github.com/acmilank22/react-app-boilerplate#readme",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.2",
    "customize-cra": "^0.2.14",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "1.18.2",
    "react-app-rewired": "^2.1.3"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "history": "^4.9.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "sanitize.css": "^10.0.0",
    "styled-components": "^4.3.2"
  }
}
