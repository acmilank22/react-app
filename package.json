{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "",
  "bugs": {
    "url": "https://github.com/acmilank22/react-app-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acmilank22/react-app-boilerplate.git"
  },
  "license": "MIT",
  "author": "",
  "main": "index.js",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "npx prettier \"**/*.{js,jsx,ts,tsx,html,vue,css,scss,json,md}\"",
    "eslint": "npx eslint --fix \"**/*.{js,jsx,html,vue}\"",
    "stylelint": "npx stylelint --fix \"**/*.{css,scss,js,jsx,vue}\"",
    "lint": "npm run prettier && npm run eslint && npm run stylelint",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-portal": "^4.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-use": "^15.2.5",
    "styled-components": "^5.1.1",
    "swr": "^0.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "commitizen": "^4.1.2",
    "customize-cra": "^1.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "htmlhint": "^0.14.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^2.1.6",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.1.2",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "proxy": ""
}
